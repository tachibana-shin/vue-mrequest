{
  "name": "vue-mrequest",
  "description": "Modern request library for vue",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "main": "./dist/vue-mrequest.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build -d",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest",
    "prepublish": "run-p test \"run\" build && bumpp"
  },
  "peerDependencies": {
    "@vueuse/core": "^11.0.3",
    "lodash.get": "^4.4.2",
    "ts-retry": "^5.0.1",
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tachibana-shin/eslint-config": "^1.1.1",
    "@tachibana-shin/eslint-config-vue": "^1.1.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/lodash.delay": "^4.1.9",
    "@types/lodash.get": "^4.4.9",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "ajv": "^8.17.1",
    "bumpp": "^9.5.2",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "lodash.delay": "^4.1.1",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "typescript": "~5.4.0",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^4.1.1",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.21"
  }
}
